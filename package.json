{
  "name": "vitestrict",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "scripts": {
    "dev": "vite --port 3333",
    "build": "vite build",
    "preview": "pnpm build && vite preview --port 4444",
    "tsc": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w -u . !{coverage/**,dist/**,**/__vite*,pnpm-lock.yaml}",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "pnpm build && playwright test",
    "review": "pnpm tsc && pnpm lint && pnpm test:run && pnpm test:e2e",
    "prod": "pnpm review && pnpm build"
  },
