  "private": true,
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "dev": "vite --port 3333",
    "build": "vite build",
    "preview": "pnpm build && vite preview --port 4444",
    "tsc": "vue-tsc --noEmit",
    "lint": "eslint .",
